{
  "expo": {
    "name": "mymix",
    "slug": "mymix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maverock24.mymix"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.maverock24.mymix",
      "permissions": [
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "WAKE_LOCK",
        "READ_MEDIA_AUDIO",
        "POST_NOTIFICATIONS",
        "READ_EXTERNAL_STORAGE",
        "MANAGE_EXTERNAL_STORAGE"
      ],
      "usesLibrary": [
        {
          "name": "androidx.car.app.app-automotive",
          "required": false
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-control",
        {
          "enableBackgroundAudio": true,
          "audioSessionCategory": "playback",
          "skipInterval": 15,
          "notificationChannel": {
            "name": "MyMix Media",
            "description": "Controls for audio playback",
            "importance": "low"
          },
          "android": {
            "enableMediaSession": true,
            "enableAutoSupport": true,
            "mediaSessionExtras": {
              "androidx.media.session.ARGUMENT_MEDIA_ATTRIBUTE_PRESENTATION_MODE_CONTENT_ITEM": "true"
            }
          },
          "ios": {
            "enableCarPlay": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "94410ac1-302d-4fa7-9f48-11066e334f8e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/94410ac1-302d-4fa7-9f48-11066e334f8e"
    }
  }
}
